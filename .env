#
# PHP source directory
#
SOURCE_DIR=./www

#
# Runtime data directory
#
DATA_DIR=./data

#
# Container Timezone
#
TZ=Asia/Shanghai

#
# Container package fetch url
#
# Can be empty, followings or others:
# mirrors.163.com
# mirrors.aliyun.com
# mirrors.ustc.edu.cn
# dl-cdn.alpinelinux.org
#
CONTAINER_PACKAGE_URL=mirrors.ustc.edu.cn

#
#ALPINE
#
ALPINE_VERSION=3.12

#
# Node
#
NODE_VERSION=10.16.1-alpine

#
# VERDACCIO
#
VERDACCIO_CONFD_DIR=./services/verdaccio/conf
VERDACCIO_CONF_FILE=./services/verdaccio/conf/config.yaml
VERDACCIO_PLUGIN_DIR=./services/verdaccio/plugins
